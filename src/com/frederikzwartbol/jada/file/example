var c = 0;

print c;

fun thrice(fn) {
  for (var i = 1; i <= 3; i = i + 1) {
    fn(i);
  }
}

thrice(fun (a) {
  var b = a + 1;

  print a+b;
});
// "2".
// "4".
// "6".

class DevonshireCream {
  serveOn() {
    return "Scones";
  }
}

print DevonshireCream;

class Bagel {}
Bagel();

print "Bagel";

class Bacon {
  eat() {
    print "Crunch crunch crunch!";
  }
}

Bacon().eat();